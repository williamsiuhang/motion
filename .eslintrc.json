{
  "env": {
    "browser": true,
    "es2021": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["prettier"],
  "extends": ["standard-with-typescript", "prettier"],
  "overrides": [],
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-misused-promises": 0,
    "@typescript-eslint/strict-boolean-expressions": 0,
    "prettier/prettier": [
      1,
      {
        "endOfLine": "auto",
        "singleQuote": true
      }
    ]
  }
}
